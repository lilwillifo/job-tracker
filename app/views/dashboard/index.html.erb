<header>
  <h1>Job Tracker</h1>
</header>
<main>
  <div>
    <h2>Jobs by Interest</h2>
    <table>
      <% (1..10).each do |num| %>
      <tr>
        <td> <%= "*" * num %> (<%= Job.count_by_level_of_interest(num) %> jobs)</td>
      </tr>
      <% end %>
    </table>
  </div>
  <div>
    <h2>Top Companies by Interest</h2>
  </div>
  <div>
  </div>
</main>
