<table>
  <tr>
    <th>Edit</th>
    <th>Delete</th>
    <th>Name</th>
    <th>Role</th>
    <th>E-mail</th>
  </tr>
  <% @company.contacts.each do |contact| %>
    <tr class=contact_<%= contact.id %>>
      <%= link_to 'Delete', company_contact_path(contact), method: :delete %>
      <ul><%= contact.name %></ul>
      <ul><%= contact.role %></ul>
      <ul><%= contact.email %></ul>
    </tr>
  <% end %>
</table>
